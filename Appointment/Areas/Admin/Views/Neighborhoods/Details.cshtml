@model NeighborhoodOperationViewModel
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="whiteBackround border">

    <h2 class="text-info">تفاصيل الحي</h2>
    <br />

    @Html.HiddenFor(m => m.Neighborhoods.Id)

    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Neighborhoods.Name" class="col-form-label"></label>
        </div>
        <div class="col-5">
            <input asp-for="Neighborhoods.Name" readonly class="form-control" />
        </div>
    </div>

    <div class="form-group row">
        <div class="col-2">

        </div>
        <div class="col-5">
            <partial name="_EditAndBackToListButtonPartial" model="@Model.Neighborhoods.Id" />
        </div>
    </div>
</div>


